<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <!--顺风车司机查找附近乘客聚合信息-->
    <bean id="hitch_driver_nearby_passenger_aggregation:hello.hitch.passenger"
          class="blossom.project.towelove.framework.flower.executor.FlowTemplate">
        <property name="beforeTrans">
            <list>
                <value>blackListFillComponent</value>
                <value>driverInfoFillComponent</value>
                <!--围栏场景下，引擎入参组装-->
                <value>fenceDriverMatchPassengerParamBuildComponent</value>
                <value>driverMatchNearbyAggregationComponent</value>
            </list>
        </property>
        <property name="trans">
            <list>
            </list>
        </property>
        <property name="afterTrans">
            <list>
            </list>
        </property>
        <property name="transEnable" value="false"/>
    </bean>

    <!--司机当前位置附近乘客订单列表信息-->
    <bean id="driver_current_location_nearby_passenger_order:hello.hitch.passenger"
          class="blossom.project.towelove.framework.flower.executor.FlowTemplate">
        <property name="beforeTrans">
            <list>
                <value>blackListFillComponent</value>
                <!--围栏场景下，引擎入参组装-->
                <value>driverInfoFillComponent</value>
                <value>fenceDriverMatchPassengerParamBuildComponent</value>
                <value>driverMatchNearbyPassengerComponent</value>
                <value>startPointDistanceCalculateComponent</value>
                <value>matchResultFilterComponent</value>
                <value>matchResultResortComponent</value>
                <!--匹配结果填充-->
                <value>matchResultFillComponent</value>
                <value>spvV2LogMetricComponent</value>
            </list>
        </property>
        <property name="trans">
            <list>
            </list>
        </property>
        <property name="afterTrans">
            <list>
            </list>
        </property>
        <property name="transEnable" value="false"/>
    </bean>

    <!--司机目的地附近乘客订单列表信息-->
    <bean id="driver_destination_nearby_passenger_order:hello.hitch.passenger"
          class="blossom.project.towelove.framework.flower.executor.FlowTemplate">
        <property name="beforeTrans">
            <list>
                <value>blackListFillComponent</value>
                <value>driverInfoFillComponent</value>
                <!--围栏场景下，引擎入参组装-->
                <value>fenceDriverMatchPassengerParamBuildComponent</value>
                <value>driverMatchNearbyPassengerComponent</value>
                <value>matchResultFilterComponent</value>
                <value>matchResultResortComponent</value>
                <!--匹配结果填充-->
                <value>matchResultFillComponent</value>
            </list>
        </property>
        <property name="trans">
            <list>
            </list>
        </property>
        <property name="afterTrans">
            <list>
            </list>
        </property>
        <property name="transEnable" value="false"/>
    </bean>

    <!--司机常用路线匹配乘客订单信息-->
    <bean id="driver_frequent_journey_match_order:hello.hitch.passenger"
          class="blossom.project.towelove.framework.flower.executor.FlowTemplate">
        <property name="beforeTrans">
            <list>
                <value>routeInfoComponent</value>
                <value>blackListFillComponent</value>
                <value>driverInfoFillComponent</value>
                <!--围栏场景下，引擎入参组装-->
                <value>fenceDriverMatchPassengerParamBuildComponent</value>
                <value>driverFrequentJourneyMatchPassengerComponent</value>
                <!-- 司机起点距离乘客起点距离 -->
                <value>startPointDistanceCalculateComponent</value>
                <!-- 司机终点距离乘客终点距离 -->
                <value>endPointDistanceCalculateComponent</value>
                <value>matchResultResortComponent</value>
                <!--匹配结果填充-->
                <value>matchResultFillComponent</value>
                <value>spvV2LogMetricComponent</value>
            </list>
        </property>
        <property name="trans">
            <list>
            </list>
        </property>
        <property name="afterTrans">
            <list>
            </list>
        </property>
        <property name="transEnable" value="false"/>
    </bean>

    <!--司机接单前再拼一单-->
    <bean id="hitch_driver_pre_match_passenger_order:hello.hitch.passenger"
          class="blossom.project.towelove.framework.flower.executor.FlowTemplate">
        <property name="beforeTrans">
            <list>
                <!--黑名单 -->
                <value>blackListFillComponent</value>
                <value>driverInfoFillComponent</value>
                <!--进行简单路径规划-->
                <value>routeInfoComponent</value>
                <!--围栏场景下，引擎入参组装-->
                <value>fenceDriverMatchPassengerParamBuildComponent</value>
                <!--召回-->
                <value>driverOrderMatchPassengerComponent</value>
                <!--匹配结果过滤-->
                <value>matchResultFilterComponent</value>
                <!--接送顺序和拼单顺路度-->
                <value>pickUpSeqAndHitchRateComponent</value>
                <!--匹配结果排序-->
                <value>matchResultResortComponent</value>
                <!--匹配结果填充-->
                <value>matchResultFillComponent</value>
                <value>spvV2LogMetricComponent</value>
            </list>
        </property>
        <property name="trans">
            <list></list>
        </property>
        <property name="afterTrans">
            <list></list>
        </property>
        <property name="transEnable" value="false"/>
    </bean>

    <!--司机接单后拼单-->
    <bean id="hitch_driver_match_passenger_order:hello.hitch.passenger"
          class="blossom.project.towelove.framework.flower.executor.FlowTemplate">
        <property name="beforeTrans">
            <list>
                <!--黑名单 -->
                <value>blackListFillComponent</value>
                <value>driverInfoFillComponent</value>
                <!--围栏场景下，引擎入参组装-->
                <value>fenceDriverMatchPassengerParamBuildComponent</value>
                <!-- 召回-->
                <value>driverOrderMatchPassengerComponent</value>
                <!--获取接驾距离-->
                <value>startPointDistanceCalculateComponent</value>
                <!--获取到终点的距离-->
                <value>endPointDistanceCalculateComponent</value>
                <!--匹配结果过滤-->
                <value>matchResultFilterComponent</value>
                <!--匹配结果排序-->
                <value>matchResultResortComponent</value>
                <!--匹配结果填充-->
                <value>matchResultFillComponent</value>
                <value>spvV2LogMetricComponent</value>
            </list>
        </property>
        <property name="trans">
            <list>
            </list>
        </property>
        <property name="afterTrans">
            <list>
            </list>
        </property>
        <property name="transEnable" value="false"/>
    </bean>

    <!--司机召回打包拼单-->
    <bean id="hitch_driver_match_passenger_package_order:hello.hitch.passenger"
          class="blossom.project.towelove.framework.flower.executor.FlowTemplate">
        <property name="beforeTrans">
            <list>
                <!-- 黑名单 -->
                <value>blackListFillComponent</value>
                <value>driverInfoFillComponent</value>
                <!--围栏场景下，引擎入参组装-->
                <value>fenceDriverMatchPassengerParamBuildComponent</value>
                <!-- 召回 -->
                <value>driverMatchPackageRecallComponent</value>
                <!-- 司机找顺风车乘客打包单结果截取+埋点 -->
                <value>driverMatchPackageCutAndMetricComponent</value>
                <!--获取接驾距离-->
                <value>startPointDistanceCalculateComponent</value>
                <!--获取到终点的距离-->
                <value>endPointDistanceCalculateComponent</value>
                <!--匹配结果填充-->
                <value>matchResultFillComponent</value>
            </list>
        </property>
        <property name="trans">
            <list>
            </list>
        </property>
        <property name="afterTrans">
            <list>
            </list>
        </property>
        <property name="transEnable" value="false"/>
    </bean>

    <!--司机起终点规划路线找单-->
    <bean id="hitch_driver_plan_route_match_passenger:hello.hitch.passenger"
          class="blossom.project.towelove.framework.flower.executor.FlowTemplate">
        <property name="beforeTrans">
            <list>
                <!--黑名单 -->
                <value>blackListFillComponent</value>
                <!--进行简单路径规划-->
                <value>routeInfoComponent</value>
                <value>driverInfoFillComponent</value>
                <!--围栏场景下，引擎入参组装-->
                <value>fenceDriverMatchPassengerParamBuildComponent</value>
                <!-- 召回-->
                <value>driverRouteMatchPassengerComponent</value>
                <!--获取接驾距离-->
                <value>startPointDistanceCalculateComponent</value>
                <!--获取到终点的距离-->
                <value>endPointDistanceCalculateComponent</value>
                <!--匹配结果排序-->
                <value>matchResultResortComponent</value>
                <!--匹配结果填充-->
                <value>matchResultFillComponent</value>
            </list>
        </property>
        <property name="trans">
            <list>
            </list>
        </property>
        <property name="afterTrans">
            <list>
            </list>
        </property>
        <property name="transEnable" value="false"/>
    </bean>

</beans>