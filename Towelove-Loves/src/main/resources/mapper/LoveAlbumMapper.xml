    <select id="selectLoveAlbums" resultType="blossom.project.towelove.loves.entity.LoveAlbum">
        select id,loves_id,title,create_time,photo_desc
        from love_album
        where loves_id = ${lovesId} and deleted = 0
        order by create_time desc
        limit ${pageNo},${pageSize}
    </select>
